@model Vidly.ViewModels.MoviesViewModel
@{
    ViewBag.Title = "ViewMovies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>All movies</h2>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                NAME
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model.Movies)
        {
            <tr>
                <td>@movie.Name</td>
            </tr>
        }
    </tbody>
</table>


